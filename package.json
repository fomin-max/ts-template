{
  "name": "gennix",
  "version": "1.0.0",
  "scripts": {
    "start": "yarn run clean && webpack serve --config webpack.dev.config",
    "build": "yarn run clean && webpack --config webpack.prod.config",
    "build:dev": "yarn run clean && webpack --watch --config webpack.dev.config",
    "build:analyze": "webpack --config webpack.analyze.config",
    "lint": "eslint src --color --ext .tsx,.ts",
    "tsc": "tsc",
    "test": "jest --passWithNoTests",
    "check": "yarn run lint && yarn run tsc && yarn run test",
    "clean": "rimraf dist/ && rimraf build/"
  },
  "devDependencies": {
    "@agilecode/eslint": "1.0.5",
    "@types/history": "^4.7.8",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.169",
    "@types/node": "^15.3.1",
    "@types/query-string": "^6.3.0",
    "@types/react": "^17.0.6",
    "@types/react-dom": "^17.0.5",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "circular-dependency-plugin": "^5.2.2",
    "css-loader": "^5.2.5",
    "css-minimizer-webpack-plugin": "^3.0.0",
    "eslint": "^7.26.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-react": "^1.1.7",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-optimize-regex": "^1.2.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^5.3.1",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^1.6.0",
    "moment-locales-webpack-plugin": "^1.2.0",
    "prettier": "^2.2.1",
    "sass": "^1.32.13",
    "sass-loader": "^11.1.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^9.2.1",
    "typescript": "^4.2.4",
    "webpack": "^5.37.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "copy-webpack-plugin": "^8.1.1",
    "history": "^5.0.0",
    "husky": "^6.0.0",
    "lodash": "^4.17.21",
    "query-string": "^7.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hot-loader": "^4.13.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn run check"
    }
  }
}
