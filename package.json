{
  "name": "ts-template",
  "version": "1.0.0",
  "scripts": {
    "start": "npm run clean && webpack-dev-server --config webpack.dev.config",
    "build": "npm run clean && webpack --config webpack.prod.config",
    "build:dev": "npm run clean && webpack --watch --config webpack.dev.config",
    "build:analyze": "webpack --config webpack.analyze.config",
    "lint": "eslint src --color --ext .tsx,.ts",
    "tsc": "tsc",
    "test": "jest --passWithNoTests",
    "check": "npm run lint && npm run tsc && npm run test",
    "clean": "rimraf dist/"
  },
  "devDependencies": {
    "@types/history": "^4.7.3",
    "@types/jest": "^24.0.25",
    "@types/lodash": "^4.14.149",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.1.5",
    "@types/react-router-dom": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^2.15.0",
    "circular-dependency-plugin": "^5.2.0",
    "css-loader": "^3.6.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^6.3.1",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-optimize-regex": "^1.1.7",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^26.0.1",
    "moment-locales-webpack-plugin": "^1.1.2",
    "sass": "^1.26.9",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "ts-loader": "^6.2.1",
    "typescript": "3.8.3",
    "webpack": "^4.41.5",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.14.0",
    "@rematch/core": "^1.4.0",
    "@types/query-string": "^3.0.32",
    "axios": "^0.19.1",
    "bootstrap-css-only": "^4.4.1",
    "connected-react-router": "^6.6.1",
    "copy-webpack-plugin": "^5.1.1",
    "core-js": "^3.6.3",
    "file-loader": "^6.0.0",
    "history": "^4.10.1",
    "husky": "^4.2.3",
    "lodash": "^4.17.15",
    "mdbreact": "^4.27.0",
    "moment": "^2.24.0",
    "node-sass": "^4.14.1",
    "query-string": "^3.0.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-hot-loader": "^4.12.18",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-svg-loader": "^3.0.3",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "reselect": "^4.0.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run check"
